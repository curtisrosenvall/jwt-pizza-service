{
  "collectCoverage": true,
  "coverageReporters": ["json-summary", "text"],
  "coverageThreshold": {
    "global": {
      "lines": 70
    }
  },
  "setupFiles": ["./jest.setup.js"],
  "globalSetup": "./globalSetup.js",
  "globalTeardown": "./globalTeardown.js",
  "testTimeout": 30000,
  "coveragePathIgnorePatterns": [
    "/node_modules/",
    "/dist/",
    "metrics.js",
    "orderMiddleware.js",
    "authMiddleware.js",
    "databaseWrapper.js",
    "factoryWrapper.js",
    "logger.js"
  ]

}